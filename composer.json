{
  "name": "saeven/laminas-aws-sdk",
  "description": "Laminas Module that allows easy integration the AWS SDK for PHP",
  "type": "library",
  "license": "Apache-2.0",
  "keywords": [
    "aws",
    "zf2",
    "amazon",
    "zend",
    "s3",
    "dynamodb",
    "ec2",
    "cloudfront",
    "sqs"
  ],
  "prefer-stable": true,
  "require": {
    "php": "^7.4",
    "aws/aws-sdk-php": "3.*",
    "laminas/laminas-filter": "^2.11.0",
    "laminas/laminas-servicemanager": "^3.7.0",
    "laminas/laminas-session": "^2.7.0",
    "laminas/laminas-view": "^2.14.0"
  },
  "require-dev": {
    "phpunit/phpunit": "7.5.20",
    "laminas/laminas-modulemanager": "^2.11.0",
    "squizlabs/php_codesniffer": "3.6.1",
    "laminas/laminas-coding-standard": "^2.3.0",
    "phpstan/phpstan": "1.1.1"
  },
  "autoload": {
    "psr-4": {
      "AwsModule\\": "src/"
    },
    "classmap": [
      "./Module.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "AwsModule\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "check": [
      "@cs"
    ],
    "cs": "vendor/bin/phpcs"
  }
}
